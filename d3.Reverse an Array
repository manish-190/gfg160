class Solution {
    public void reverseArray(int arr[]) {
        // code here
       // Arrays.sort(arr);
        int k = 0;
        
        int n =  arr.length;
        for(int i = arr.length-1 ; i > k; i--){
            //System.out.print(arr[i]);
             if(k < i){
            int temp = arr[i];
            arr[i] = arr[k];
            arr[k] = temp;
            k++;
             }
        }
    }
}
